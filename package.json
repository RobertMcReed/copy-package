{
  "name": "cp-dep",
  "version": "3.0.1",
  "description": "A CLI to view or copy dependencies to a new project",
  "main": "./index.js",
  "preferGlobal": true,
  "bin": {
    "cp-dep": "./index.js"
  },
  "scripts": {
    "test": "jest -i --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "watch": "jest -i --coverage --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RobertMcReed/cp-dep.git"
  },
  "keywords": [
    "npm",
    "copy",
    "package",
    "cli",
    "dependencies",
    "devDependencies"
  ],
  "author": "Robert Reed",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RobertMcReed/cp-dep/issues"
  },
  "homepage": "https://github.com/RobertMcReed/cp-dep#readme",
  "dependencies": {
    "command-line-args": "^5.0.2",
    "command-line-usage": "^4.1.0",
    "copy-paste": "^1.3.0",
    "fs-extra": "^5.0.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^4.18.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "istanbul": "^0.4.5",
    "jest": "^22.4.2"
  }
}
